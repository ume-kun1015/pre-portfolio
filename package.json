{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "My impressive Nuxt.js project",
  "author": "romukey",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon app/server/index.ts --watch server",
    "build": "nuxt-ts build",
    "build:copy": "cp -R .nuxt/dist functions/ssr/dist",
    "start": "cross-env NODE_ENV=production ts-node app/server/index.ts",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "functions:ssr:clean": "rm -rf functions/ssr && mkdir -p functions/ssr",
    "firebase:functions:config:set": "firebase functions:config:set"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "cross-env NODE_ENV=production eslint"
    ]
  },
  "browserslist": [
    "> 0.1% in JP",
    "ie >= 10",
    "android >= 4.0"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.16",
    "@fortawesome/free-brands-svg-icons": "5.8.0",
    "@fortawesome/free-regular-svg-icons": "5.8.0",
    "@fortawesome/free-solid-svg-icons": "5.8.0",
    "@fortawesome/vue-fontawesome": "0.1.6",
    "@nuxt/typescript-runtime": "0.3.5",
    "@nuxtjs/axios": "5.3.6",
    "@nuxtjs/dotenv": "1.3.0",
    "apicache": "1.4.0",
    "cross-env": "5.2.0",
    "express": "4.16.4",
    "nuxt": "2.11.0",
    "nuxt-fontawesome": "0.4.0",
    "nuxt-property-decorator": "2.3.0",
    "superagent": "5.1.0",
    "ts-node": "8.3.0"
  },
  "devDependencies": {
    "@nuxt/typescript": "2.8.1",
    "@nuxt/typescript-build": "0.5.4",
    "@nuxtjs/style-resources": "0.1.1",
    "@typescript-eslint/eslint-plugin": "2.13.0",
    "@typescript-eslint/parser": "2.13.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-smart-webpack-import": "1.5.0",
    "eslint": "5.9.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-vue": "5.0.0",
    "firebase-tools": "^6.5.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "node-sass": "4.10.0",
    "nodemon": "1.18.7",
    "npm-check-updates": "4.0.1",
    "prettier": "1.14.3",
    "sass-loader": "7.1.0",
    "stylelint": "9.9.0",
    "stylelint-config-prettier": "4.0.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-prettier": "1.0.5",
    "stylelint-scss": "3.4.0",
    "stylelint-webpack-plugin": "0.10.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
